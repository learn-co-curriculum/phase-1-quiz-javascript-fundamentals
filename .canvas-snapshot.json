      "correct_comments": "",
      "incorrect_comments": "",
      "neutral_comments": "",
      "correct_comments_html": "",
      "incorrect_comments_html": "",
      "neutral_comments_html": "<p><strong>Source/s: </strong><a class=\"inline_disabled\" href=\"https://learning.flatironschool.com/courses/3297/pages/review-conditionals?module_item_id=143559\" target=\"_blank\">Review: Conditionals</a></p>",
      "answers": [
        {
          "id": "4200",
          "text": "false",
          "comments": "",
          "comments_html": "<p>Correct!</p>\n<p>If the value of age is less than 18, the value of isAdult is false.</p>",
